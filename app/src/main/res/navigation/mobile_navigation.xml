<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_history">

    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.final_main.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_historyfragment" />

    <fragment
        android:id="@+id/nav_contactus"
        android:name="com.example.final_main.ContactUsFragment"
        android:label="@string/contact_us"
        tools:layout="@layout/fragment_contact_us" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.final_main.SettingsFragment"
        tools:layout="@layout/fragment_settings"/>

    <fragment android:id="@+id/changePassword"
        android:name="com.example.final_main.ChangePasswordFragment"
        android:label="Change Password"
        tools:layout="@layout/fragment_change_password"/>
    <fragment
        android:id="@+id/nav_editprofile"
        android:name="com.example.final_main.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_edit_profile" />

    <fragment
        android:id="@+id/nav_aboutus"
        android:label="About us"
        android:name="com.example.final_main.AboutusFragment"
        tools:layout="@layout/fragment_aboutus"
        />

    <fragment
        android:id="@+id/nav_privacypolicy"
        android:label="Privacy"
        android:name="com.example.final_main.PrivacyPolicyFragment"
        tools:layout="@layout/fragment_privacy_policy"
        />

    <fragment
        android:id="@+id/nav_termsandcondition"
        android:label="Terms and condition"
        android:name="com.example.final_main.TermsandConditionFragment"
        tools:layout="@layout/fragment_termsand_condition"/>
</navigation>